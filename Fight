
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Gunfight Arena | Legend HandlesYT", "BloodTheme")

local Tab = Window:NewTab("Main-Features")




local Tab1 = Window:NewTab("Settings")

local Section = Tab:NewSection("Main-Features")




local CredSection = Tab1:NewSection("Settings")


Section:NewToggle("Enable Player Hitbox (Small)", "Enable Player Hitbox", function(Value)
a = Value
        while a do task.wait(2)
        pcall(function()
for _, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "HumanoidRootPart" and v:IsA("Part") and v.Parent ~= game.Players.LocalPlayer.Character then
        v.Size = Vector3.new(10,10,10)
        v.Transparency = 0.8
        v.Material = "Neon"
        v.BrickColor = BrickColor.new("Really red")
        v.CanCollide = false
    end
end
end)
end
    end)


Section:NewToggle("Enable Player Hitbox (Medium)", "Enable Player Hitbox", function(Value)
a = Value
        while a do task.wait(2)
        pcall(function()
for _, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "HumanoidRootPart" and v:IsA("Part") and v.Parent ~= game.Players.LocalPlayer.Character then
        v.Size = Vector3.new(15, 15, 15)
        v.Transparency = 0.8
        v.Material = "Neon"
        v.BrickColor = BrickColor.new("Really red")
        v.CanCollide = false
    end
end
end)
end
    end)


Section:NewToggle("Enable Player Hitbox (Big)", "Enable Player Hitbox", function(Value)
a = Value
        while a do task.wait(2)
        pcall(function()
for _, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "HumanoidRootPart" and v:IsA("Part") and v.Parent ~= game.Players.LocalPlayer.Character then
        v.Size = Vector3.new(25, 25, 25)
        v.Transparency = 0.8
